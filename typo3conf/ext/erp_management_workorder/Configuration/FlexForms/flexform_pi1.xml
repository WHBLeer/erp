<T3DataStructure>
	<meta><langDisable>1</langDisable></meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms><sheetTitle>基本配置</sheetTitle></TCEforms>
				<type>array</type>
				<el>
					<!-- View -->
					<switchableControllerActions>
						<TCEforms>
							<label>加载模块</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0">
										<numIndex index="0">用户工单管理</numIndex>
										<numIndex index="1">Workorder-&gt;qlist;Workorder-&gt;show;Workorder-&gt;new;Workorder-&gt;create;Workorder-&gt;edit;Workorder-&gt;update;Workorder-&gt;delete;Workorder-&gt;submit</numIndex>
